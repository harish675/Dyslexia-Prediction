
<h1>Sign Up</h1>

<form action="/user/create" method="POST">
    <label for="firstName">First Name</label>
    <input type="text" name="firstName" placeholder="Your First Name" id="firstName" required>
    <br>
    <label for="lastName">Last Name</label>
    <input type="text" name="lastName"  placeholder="Entre Last Name" id="lastName"required>
    <br>
    <label for="dob">Birthday</label>
    <input type="date" name="dob" id="dob" required>
    <br>
    <label for="age">Age</label>
    <input type="number" name="age" id="age" placeholder="eg.12" required>
    <br>
    <label for="mobileNumber">Mobile Number</label>
    <input type="number"  name="mobileNumber" id="mobileNumber" placeholder="eg.7666660636" required>
    <br>
    <label for="email">Email</label>
    <input type="email" name="email" id="email" placeholder="email"  required>
    <br>
    <label for="password">Password</label>
    <input type="password" name="password" id="password" placeholder="password" required>
    <br>
    <label for="confirm-password">Confirm-Password</label>
    <input type="password" name="confirm-password" id="confirm-password" placeholder="entre password again" required>
    <br>
    <input type="submit" value="Sign Up">
    <button type="login">LogIn</button>
</from>